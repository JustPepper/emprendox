	div.topBar
		header.u-flex-center
			div.topBar-hamburguer
				svg.u-icon
					use(xlink:href="#menu")
			div.topBar-logo
				a(href="/")
					img(src="img/emprendox-logo.svg", alt="Emprendox, Negocios y Franquicias")
			nav.topBar-menu
				ul
					li
						a.u-flex-center(href="negocios.html") NEGOCIOS
					li
						a.u-flex-center(href="Franquicias.html") FRANQUICIAS
			div.topBar-buttons
				div.topBar-buttons--publications
					a.u-flex-center(href="publicar.html") Publicar
				
				//- If the user is not logged
				div.topBar-buttons--login
					a#login-link.u-flex-center(href="#")
						svg.u-icon
							use(xlink:href="#login")
						span Iniciar sesión 
				
				//- If the user is logged
				div.topBar-buttons--logged.u-flex-center
					span Josue Maridueña
					div
						img(src="img/default-avatar.jpg", alt="")

	div.sideNav
		div.sideNav-userInfo
			div
				img(src="img/default-avatar.jpg", alt="")
			span Josue Maridueña
		hr
		nav.sideNav-userNav
			ul
				li
					a(href="perfil.html")	Perfil
				li
					a(href="mensajes.html")	Mensajes
				li
					a(href="anuncios.html")	Anuncios
				li
					a(href="favoritos.html")	Favoritos
				li
					a(href="alertas.html")	Alertas
		hr
		div.sideNav-logout
			a(href="#") Cerrar sesión

	div.siteWrapper
			
		div.searchBox.u-flex-center
			div.searchBox-wrapper
				div.searchBox-tabs
					div.u-flex-center
						label Negocios
					div.u-flex-center
						label Franquicias
				div.searchBox-input
					label
						svg
							use(xlink:href="#lens")
					input(type="text" placeholder="Tipo, Ciudad, Precio")

		footer
			div.container
				div.footerNav
					div.footerNav-list
						h2  Empresa
							svg
								use(xlink:href="#plus")
						nav
							ul
								li  
									a(href="#")  Quiénes somos
								li  
									a(href="#")  Acerca de
								li  
									a(href="#")  Contactar
					div.footerNav-list
						h2  Negocios
							svg
								use(xlink:href="#plus")
						nav
							ul
								li  
									a(href="#")  Negocio en marcha
								li  
									a(href="#")  Propiedad
								li  
									a(href="#")  Activos
								li  
									a(href="#")  Franquicias
					div.footerNav-list
						h2  Sugerencias
							svg
								use(xlink:href="#plus")
						nav
							ul
								li  
									a(href="#")  Buscar negocio
								li  
									a(href="#")  Mapa del sitio
					div.footerNav-list
						h2  Políticas
							svg
								use(xlink:href="#plus")
						nav
							ul
								li  
									a(href="#")  Política de privacidad
								li  
									a(href="#")  Política de uso de sitio web
				
				div.footerAbout
					img(src="img/emprendox-logo.svg", alt="")
					p
						|  Sotheby's International Realty Canada, Brokerage.
						|  Sotheby's International Realty® is a registered trademark licensed to Sotheby's International Realty Affiliates, Inc. Each Office Is Independently Owned And Operated.
					nav
						ul
							li
								svg
									use(xlink:href="#facebook")
							li
								svg
									use(xlink:href="#twitter")
							li
								svg
									use(xlink:href="#google")
				hr
				p.copy-right
					|  © Emprendox
	div.loginPopup
		div.loginPopup-wrapper
			span Iniciar sesión a tu perfil
			div.loginPopup-media
				a(href="#")
					svg
						use(xlink:href="#facebook")
					| Iniciar con facebook
				a(href="#")
					svg
						use(xlink:href="#google")
					| Iniciar con google
				a(href="#")
					svg
						use(xlink:href="#twitter")
					| Iniciar con twitter
				a(href="#")
					svg
						use(xlink:href="#linkedin")
					| Iniciar con linkedin
			div.loginPopup-form
				form(action="")
					p
						label
							svg
								use(xlink:href="#mail")
						input(type="text" placeholder="Correo electrónico" name="correo")
					p
						label
							svg
								use(xlink:href="#password")
						input(type="text" placeholder="Contraseña" name="contraseña")
					button Iniciar sesión
					a.password(href="#") Olvidé mi contraseña
			div.loginPopup-signUp
				h2  ¿No tienes cuenta?
				a(href="registro.html")  Regístrate
	
	script(src="//code.jquery.com/jquery-1.11.3.min.js")
